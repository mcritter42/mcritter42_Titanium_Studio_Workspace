function doClick(e) {
    $.menu.open();
}

$.AgendaButton.AddEventListener('click', function(e) {
if (e.source.child_controller) {
      controller = Alloy.createController(e.source.child_controller);
      $.Container.open(controller.Agenda());
    }
});

$.MapButton.AddEventListener('click', function(e) {
if (e.source.child_controller) {
      controller = Alloy.createController(e.source.child_controller);
      $.Container.open(controller.Map());
    }
});

$.SettingsButton.AddEventListener('click', function(e) {
if (e.source.child_controller) {
      controller = Alloy.createController(e.source.child_controller);
      $.Container.open(controller.Settings());
    }
});
$.index.open();
