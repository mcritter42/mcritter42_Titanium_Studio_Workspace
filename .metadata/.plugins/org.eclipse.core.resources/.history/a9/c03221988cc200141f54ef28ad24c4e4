var args = arguments[0] || {};

var tbl_data = 
[

];

for(var i =0; i <1; i++){
	var tvr = Ti.UI.createTableViewRow({
		height: 'auto',
		backgroundColor: 'transparent'
	});
	
	var view = Titanium.UI.createView({
  backgroundColor:'white',
  left: '10',
  right: '10',
  height: 'auto', 
  bottom: '20'
});

var lable = Ti.UI.createLabel({
	title: 'row' + i, 
	backgroundColor: 'grey'
});

tvr.add(view);

view.add(lable);

tbl_data.push(tvr);

}

$.TV.setData(tbl_data);

