var tbl_data = [
	1,
	2, 
	3, 
	4, 
	5, 
];

for(var i =0; i < tbl_data.Length; i++){
	var tvr = Ti.UI.createTableViewRow({
		height: 'auto',
		backgroundColor: 'transparent'
	});
	
	var view = Titanium.UI.createView({
  		backgroundColor:'white',
  		left: '10',
  		right: '10',
  		height: '100', 
  		bottom: '20'
});

tvr.add(view);

tbl_data.push(tvr);
}

$.TV.setData(tbl_data);

function doClick(e) {
    $.menu.open();
}
$.index.open();
